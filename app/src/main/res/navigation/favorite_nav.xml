<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/favoriteFragment">

    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.example.githubuser.ui.FavoriteFragment"
        android:label="@string/favorite_title"
        tools:layout="@layout/fragment_favorite">
        <action
            android:id="@+id/action_favoriteFragment_to_userDetailFragment2"
            app:destination="@id/userDetailFragment2" />
    </fragment>
    <fragment
        android:id="@+id/userDetailFragment2"
        android:name="com.example.githubuser.ui.UserDetailFragment"
        tools:layout="@layout/fragment_user_detail" />
</navigation>